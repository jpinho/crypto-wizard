{
  "name": "crypto-web",
  "description": "A web client that allows users to make guesses on whether the market price of Bitcoin (BTC/USD) will be higher or lower after one minute",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf __sapper__",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "npm run build && node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "tdd": "npm run test -- --watch --verbose",
    "test": "LOG_LEVEL=silent jest --runInBand"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/runtime": "^7.0.0",
    "@pyoner/svelte-ts-preprocess": "^1.3.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "babel-jest": "^26.6.3",
    "http-proxy-middleware": "^1.0.6",
    "jest": "^26.6.3",
    "jest-transform-svelte": "^2.1.1",
    "nodemon": "^2.0.7",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.3.0",
    "ts-jest": "^26.5.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  }
}
